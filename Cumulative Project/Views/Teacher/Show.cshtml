@model Cumulative_Project.Models.Teacher

@{
    ViewBag.Title = "Show";
}

<h1>@Model.Name @Model.LastName</h1>
<p class="caption">Teacher</p>
<hr />

<h2>General Information</h2>
<table class="table">
    <tr>
        <th>Employee Number</th>
        <th>Hire Date</th>
        <th>Salary (/hr)</th>
    </tr>
    <tr>
        <td>@Model.Id</td>
        <td>@Model.HireDate</td>
        <td>@Model.Salary</td>
    </tr>
</table>

<a class="navlink" href="/Teacher/List">Back</a>
